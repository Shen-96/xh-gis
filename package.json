{
    "name": "xh-gis",
    "version": "1.0.15",
    "description": "Bundle package that installs @xh-gis/engine and @xh-gis/widgets",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "scripts": {
        "build": "tsc && node copy-assets.mjs && node compress.mjs",
        "build:dev": "tsc && node copy-assets.mjs",
        "build:packages": "pnpm -r --filter='!xh-gis-sandcastle' build",
        "prepublishOnly": "pnpm build:packages && pnpm build",
        "publish:all": "pnpm -r publish"
    },
    "keywords": [
        "gis",
        "cesium",
        "3d",
        "maps",
        "typescript",
        "webgl"
    ],
    "author": "Xiaohu.Shen",
    "license": "MIT",
    "dependencies": {
        "@xh-gis/engine": "^1.0.8",
        "@xh-gis/widgets": "^1.0.1"
    },
    "devDependencies": {
        "terser": "^5.43.1",
        "typescript": "^5.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Shen-96/xh-gis.git"
    },
    "homepage": "https://github.com/Shen-96/xh-gis#readme",
    "bugs": {
        "url": "https://github.com/Shen-96/xh-gis/issues"
    }
}
